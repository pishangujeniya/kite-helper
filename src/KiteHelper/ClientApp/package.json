{
  "name": "kitehelper",
  "version": "2.0.0",
  "scripts": {
    "install:packages": "npm install --legacy-peer-deps --location=local",
    "ng": "ng",
    "prestart": "node aspnetcore-https",
    "start": "npm run install:packages && npm run lint:fix && run-script-os",
    "start:windows": "ng serve --port 44453 -o --host 0.0.0.0 --disable-host-check --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
    "start:default": "ng serve --port 44453 -o --host 0.0.0.0 --disable-host-check --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
    "lint": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:styles": "stylelint ./src/**/*.scss",
    "lint:ci": "npm run lint && npm run lint:styles",
    "build": "ng build",
    "build:ssr": "ng run KiteHelper:server:dev",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "build:prod": "npm run lint:fix && ng build --configuration production"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.8",
    "@angular/cdk": "^14.2.6",
    "@angular/common": "^14.2.8",
    "@angular/compiler": "^14.2.8",
    "@angular/core": "^14.2.8",
    "@angular/forms": "^14.2.8",
    "@angular/platform-browser": "^14.2.8",
    "@angular/platform-browser-dynamic": "^14.2.8",
    "@angular/platform-server": "^14.2.8",
    "@angular/router": "^14.2.8",
    "@nebular/eva-icons": "^9.1.0-rc.6",
    "@nebular/theme": "^9.1.0-rc.6",
    "bootstrap": "^5.1.3",
    "eva-icons": "^1.1.3",
    "ionicons": "^2.0.1",
    "jquery": "^3.6.0",
    "nebular-icons": "^1.1.0",
    "oidc-client": "^1.11.5",
    "popper.js": "^1.16.0",
    "run-script-os": "^1.1.6",
    "rxjs": "~7.5.5",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.7",
    "@angular-eslint/builder": "13.3.0",
    "@angular-eslint/eslint-plugin": "13.3.0",
    "@angular-eslint/eslint-plugin-template": "13.0.0",
    "@angular-eslint/schematics": "13.3.0",
    "@angular-eslint/template-parser": "13.3.0",
    "@angular/cli": "^14.2.7",
    "@angular/compiler-cli": "^14.2.8",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^17.0.29",
    "@typescript-eslint/eslint-plugin": "4.28.2",
    "@typescript-eslint/parser": "4.28.2",
    "eslint": "^7.26.0",
    "eslint-plugin-import": "latest",
    "eslint-plugin-jsdoc": "latest",
    "eslint-plugin-prefer-arrow": "latest",
    "jasmine-core": "~4.1.0",
    "karma": "~6.3.19",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "^1.7.0",
    "stylelint": "7.13.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "~4.6.3"
  }
}
